# 기본 설정
NODE_ENV=development
PORT=4000
LOG_DIR=logs

# 데이터베이스 설정
DATABASE_URL="postgresql://postgres:password@localhost:5432/mindroute?schema=public"

# JWT 설정
JWT_SECRET=your_secure_jwt_secret_key
JWT_EXPIRATION=24h

# 암호화 설정 (API 키 등 민감 정보 암호화용)
ENCRYPTION_KEY=your_secure_encryption_key_32_chars
ENCRYPTION_IV=your_secure_iv

# CORS 설정
CORS_ORIGIN=http://localhost:3000

# 로그 레벨
LOG_LEVEL=debug
